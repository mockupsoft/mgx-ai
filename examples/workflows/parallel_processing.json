{
  "name": "Parallel Processing Workflow",
  "description": "A workflow that processes data in parallel branches before merging results",
  "config": {},
  "timeout_seconds": 5400,
  "max_retries": 2,
  "variables": [
    {
      "name": "input_data",
      "data_type": "json",
      "is_required": true,
      "description": "Input data to be processed in parallel"
    },
    {
      "name": "num_workers",
      "data_type": "string",
      "is_required": false,
      "default_value": "4",
      "description": "Number of parallel workers"
    }
  ],
  "steps": [
    {
      "name": "data_preparation",
      "step_type": "task",
      "step_order": 1,
      "config": {
        "description": "Prepare and split data for parallel processing"
      },
      "depends_on_steps": [],
      "meta_data": {
        "category": "preparation"
      }
    },
    {
      "name": "branch_a",
      "step_type": "parallel",
      "step_order": 2,
      "config": {
        "description": "Process first branch of data",
        "timeout": 1800
      },
      "depends_on_steps": ["data_preparation"],
      "meta_data": {
        "category": "processing"
      }
    },
    {
      "name": "branch_b",
      "step_type": "parallel",
      "step_order": 2,
      "config": {
        "description": "Process second branch of data",
        "timeout": 1800
      },
      "depends_on_steps": ["data_preparation"],
      "meta_data": {
        "category": "processing"
      }
    },
    {
      "name": "branch_c",
      "step_type": "parallel",
      "step_order": 2,
      "config": {
        "description": "Process third branch of data",
        "timeout": 1800
      },
      "depends_on_steps": ["data_preparation"],
      "meta_data": {
        "category": "processing"
      }
    },
    {
      "name": "merge_results",
      "step_type": "task",
      "step_order": 3,
      "config": {
        "description": "Merge results from all parallel branches"
      },
      "depends_on_steps": ["branch_a", "branch_b", "branch_c"],
      "meta_data": {
        "category": "aggregation"
      }
    }
  ]
}
